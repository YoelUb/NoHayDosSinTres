<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <display-name>NoHayDosSinTres</display-name>

    <!-- Mapeo de servlets -->
    <servlet>
        <servlet-name>FormularioServlet</servlet-name>
        <servlet-class>servidor.FormularioServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FormularioServlet</servlet-name>
        <url-pattern>/FormularioServlet</url-pattern>
    </servlet-mapping>

    <!-- ConfiguraciÃ³n del bienvenido -->
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
